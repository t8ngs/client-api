{
  "name": "@t8ngs/client-api",
  "description": "Browser and API testing client for t8ngs. Built on top of Playwright",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.16.0"
  },
  "main": "./dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "!dist/bin",
    "!dist/tests_helpers",
    "!dist/tests"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/src/types.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli build",
    "precompile": "npm run lint && npm run clean",
    "compile": "tsup-node && tsc --emitDeclarationOnly --declaration",
    "build": "npm run compile",
    "version": "npm run build",
    "prepublishOnly": "npm run build",
    "release": "release-it",
    "quick:test": "node --import=ts-node-maintained/register/esm --enable-source-maps bin/test.ts",
    "prepare": "husky",
    "commitlint": "commitlint --edit",
    "commit": "cz"
  },
  "devDependencies": {
    "@arapucajs/eslint-config": "^1.1.2",
    "@arapucajs/prettier": "^1.1.1",
    "@arapucajs/tsconfig": "^1.0.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@swc/core": "^1.15.0",
    "@t8ngs/assert": "^1.0.0",
    "@t8ngs/expect": "^1.1.0",
    "@t8ngs/fs": "^1.0.0",
    "@t8ngs/openapi": "^1.0.2",
    "@t8ngs/runner": "^1.0.1",
    "@types/node": "^24.10.0",
    "@types/qs": "^6.9.18",
    "@types/set-cookie-parser": "^2.4.10",
    "c8": "^10.1.3",
    "cheerio": "^1.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^7.0.0",
    "eslint": "^9.26.0",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "qs": "^6.14.0",
    "ts-node-maintained": "^10.9.5",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@poppinss/hooks": "^7.2.5",
    "@poppinss/macroable": "^1.0.4",
    "@types/superagent": "^8.1.9",
    "cookie": "^1.0.2",
    "set-cookie-parser": "^2.7.1",
    "superagent": "^10.2.1"
  },
  "peerDependencies": {
    "@t8ngs/assert": "^1.0.0 || ^3.0.0 || ^4.0.0",
    "@t8ngs/openapi": "^1.0.2",
    "@t8ngs/runner": "^1.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@t8ngs/assert": {
      "optional": true
    },
    "@t8ngs/openapi": {
      "optional": true
    }
  },
  "homepage": "https://github.com/t8ngs/client-api#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t8ngs/client-api.git"
  },
  "bugs": {
    "url": "https://github.com/t8ngs/client-api/issues"
  },
  "keywords": [
    "playwright",
    "browser-tests",
    "tests",
    "e2e",
    "api-tests"
  ],
  "author": "Jefte Costa <jefteamorim@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "tsup": {
    "entry": [
      "./index.ts",
      "./src/types.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": false,
    "target": "esnext"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "prettier": "@arapucajs/prettier",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  }
}
